NAME	=	mprintf.a

CC = clang

HEAD	=	src/mprintf.h

SRCS	=	mp_parcer.c \
			mp_putstr.c \
			
SRC_DIR = $(addprefix src/, $(SRCS))

OBJ		= $(patsubst src/%.c, obj/%.o, $(SRC_DIR))

all: $(OBJ_DIR)

$(NAME):	&(SRCS)